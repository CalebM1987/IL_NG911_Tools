{
    "tools": [
        {
            "name": "AddPreConfiguredCADVendorFields",
            "params": [
                {
                    "name": "cad_path",
                    "displayName": "CAD Table Path",
                    "datatype": "GPTableView"
                },
                {
                    "name": "featureType",
                    "displayName": "911 Feature Type",
                    "datatype": "GPString",
                    "filterList": ["ADDRESS_POINTS", "ROAD_CENTERLINE" ]
                },
                {
                    "name": "vendor",
                    "displayName": "CAD Vendor",
                    "datatype": "GPString",
                    "filterList": [ "TRITECH", "TYLER", "WTH" ]
                }
            ]
        },
        {
            "name": "AddCADVendorFields", 
            "params": [
                {
                    "name": "featureType",
                    "displayName": "911 Feature Type",
                    "datatype": "GPString",
                    "filterList": [
                        "ADDRESS_POINTS", 
                        "ROAD_CENTERLINE",
                        "PSAP",
                        "ESB",
                        "ESB_EMS",
                        "ESB_FIRE",
                        "ESB_LAW",
                        "PROVISIONING_BOUNDARY"
                    ]
                },
                {
                    "name": "vendor",
                    "displayName": "CAD Vendor",
                    "datatype": "GPString",
                    "filterList": [ "TRITECH", "TYLER", "WTH" ]
                },
                {
                    "name": "cad_fields",
                    "displayName": "CAD Fields",
                    "datatype": "GPValueTable",
                    "parameterDependencies": ["cad_path"],
                    "valueTable": {
                        "fields": [
                            {
                                "name": "Field",
                                "datatype": "Field"
                            },
                            {
                                "name": "Expression",
                                "datatype": "GPString"
                            }
                        ],
                        "filters": [
                            { 
                                "type": "Field",
                                "list": ["Text", "Short", "Long" ]
                            }
                        ]
                    }
                }
            ]
        },
        {
            "name": "AddOverlayAttributes",
            "params": [
                {
                    "name": "target_table",
                    "displayName": "Target Table",
                    "datatype": "GPFeatureLayer"
                },
                {
                    "name": "target_field",
                    "displayName": "Target Field",
                    "datatype": "Field",
                    "parameterDependencies": ["target_table"]
                },
                {
                    "name": "source_table",
                    "displayName": "Source Table",
                    "datatype": "GPFeatureLayer"
                },
                {
                    "name": "source_field",
                    "displayName": "Source Field",
                    "datatype": "Field",
                    "parameterDependencies": ["source_table"],
                    "multiValue": true
                }
            ]
        },
        {
            "name": "AddCustomFields",
            "params": [
                {
                    "name": "target_table",
                    "displayName": "Target Table",
                    "datatype": "GPFeatureLayer"
                },
                {
                    "name": "custom_fields",
                    "displayName": "Custom Fields",
                    "datatype": "GPValueTable",
                    "parameterDependencies": ["target_table"],
                    "valueTable": {
                        "fields": [
                            {
                                "name": "Field",
                                "datatype": "Field"
                            },
                            {
                                "name": "Expression",
                                "datatype": "GPString"
                            }
                        ],
                        "filters": [
                            { 
                                "type": "Field",
                                "list": ["Text" ]
                            }
                        ]
                    }
                }
                
            ]
        }
    ]
}